{
  "hosting": {
    "firebase": "your-firebase",
    "public": "build/default",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**/!(*.js|*.html|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
      "destination": "/index.html"
    }]
  }
}
